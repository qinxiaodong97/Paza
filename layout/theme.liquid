        {% render 'pixels' %}
        {%- if settings.favicon != blank -%}
            <link rel="icon" type="image/png" href="{{ settings.favicon | image_url: width: 32, height: 32 }}">
        {%- endif -%}
        <title>
            {{ page_title }}
            {%- if current_tags %}
                &ndash; tagged "{{ current_tags | join: ', ' }}"
            {%- endif -%}
            {%- if current_page != 1 %}
                &ndash; Page {{ current_page -}}
            {%- endif -%}
            {%- unless page_title contains shop.name %}
                &ndash; {{ shop.name -}}
            {%- endunless -%}
        </title>
        {% if page_description %}
            <meta name="description" content="{{ page_description | escape }}">
        {% endif %}
        {% render 'meta-tags' %}
        {% comment %} PRELOAD FONTS {% endcomment %}
        {% render 'font-preload' %}
        {% render 'font-css' %}
        {%- if request.design_mode or template contains 'cart' or template contains 'checkout' -%}
            {{ content_for_header }}
        {%- else -%}
            {% capture content_for_header2 %}{{ content_for_header }}{% endcapture %}
            {{ content_for_header2 | replace: ' defer="defer" ', ' type="lazyload_int" ' | replace: " src=", " data-src="| replace: "DOMContentLoaded", "w3-DOMContentLoaded" | replace: "window.addEventListener('load', asyncLoad, false);", "window.addEventListener('w3-DOMContentLoaded', asyncLoad, false);" | replace: "addEventListener('load', prefetchAssets);", "addEventListener('w3-DOMContentLoaded', prefetchAssets);" }}
            <script>var trekkie=[];trekkie.integrations=!0;window.BOOMR={},window.BOOMR.version=true;</script>
        {%- endif -%}
        {% render 'base-css' %}
        <link rel="stylesheet" data-href="{{ 'base.css' | asset_url }}">
        <link rel="stylesheet" data-href="{{ 'wheelio.css' | asset_url }}">
        <link rel="stylesheet" data-href="{{ 'policies.css' | asset_url }}">
        <link rel="stylesheet" data-href="{{ 'button-round-shadow.css' | asset_url }}">
        <script type="lazyload_int" data-src="{{ 'global.js' | asset_url }}"></script>
        <script type="lazyload_int" data-src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.11.3/gsap.min.js" integrity="sha512-gmwBmiTVER57N3jYS3LinA9eb8aHrJua5iQD7yqYCKa5x6Jjc7VDVaEA0je0Lu0bP9j7tEjV3+1qUm6loO99Kw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script type="lazyload_int" data-src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.11.3/ScrollTrigger.min.js" integrity="sha512-tHvUcUQYi35zApvAa5TNR3fNhBQkVtkmWKfacnpxmix4/VTGz1lGjIVyLMp7gLgAvg+aKJjnVKokXJNS5GZ+/g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script type="lazyload_int" data-src="https://unpkg.com/embla-carousel/embla-carousel.umd.js"></script>
        <script type="lazyload_int" data-src="https://unpkg.com/embla-carousel-autoplay/embla-carousel-autoplay.umd.js"></script>
        <script type="lazyload_int" data-src="https://unpkg.com/embla-carousel-class-names/embla-carousel-class-names.umd.js"></script>
        {% if template.name == 'index' %}
            <script type="lazyload_int" data-src="{{ 'hero.js' | asset_url }}"></script>
            <script type="lazyload_int" data-src="{{ 'index.js' | asset_url }}"></script>
            {% comment %} <script src="{{ 'reviews.js' | asset_url }}" defer></script> {% endcomment %}
        {% endif %}
        {{ shop.metafields.loox["global_html_head"] }}
        {% render 'wsg-custom' %}
        {% render 'custom-css' %}
        {% render 'app-block' %}
        {% include 'dataLayer-allPages' %}
    </head>
    <body class="gradient">
      <!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-K4R55QDP"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->
        <!-- Google Tag Manager (noscript) -->
        <noscript>
            <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-59TPF5D" height="0" width="0" style="display:none;visibility:hidden"></iframe>
        </noscript>
        <!-- End Google Tag Manager (noscript) -->
        <linearGradient id="my-cool-gradient" x2="1" y2="1">
            <stop offset="0%" stop-color="#447799" />
            <stop offset="50%" stop-color="#224488" />
            <stop offset="100%" stop-color="#112266" />
        </linearGradient>
        {% comment %}
            <a class="skip-to-content-link button visually-hidden" href="#MainContent">
                {{ 'accessibility.skip_to_text' | t }}
            </a>
        {% endcomment %}
        <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
            {% section 'header' %}
            {% section 'menu' %}
            {% section 'cart' %}
            <div class="cart-overlay"></div>
            {{ content_for_layout }}
        </main>
        {% section 'footer' %}
        <ul hidden>
            <li id="a11y-refresh-page-message">
                {{ 'accessibility.refresh_page' | t }}
            </li>
            <li id="a11y-new-window-message">
                {{ 'accessibility.link_messages.new_window' | t }}
            </li>
        </ul>
        <script>
            window.templateName = '{{ template.name }}';
            console.log('template name', templateName);
            window.shopUrl = '{{ request.origin }}';
            window.routes = {
                cart_add_url: '{{ routes.cart_add_url }}',
                cart_change_url: '{{ routes.cart_change_url }}',
                cart_update_url: '{{ routes.cart_update_url }}',
                cart_url: '{{ routes.cart_url }}',
                predictive_search_url: '{{ routes.predictive_search_url }}'
            };{% comment %}window.cartStrings = {
                error: `{{ 'sections.cart.cart_error' | t }}`,
                quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`
            };
            window.variantStrings = {
                addToCart: `{{ 'products.product.add_to_cart' | t }}`,
                soldOut: `{{ 'products.product.sold_out' | t }}`,
                unavailable: `{{ 'products.product.unavailable' | t }}`,
                unavailable_with_option: `{{ 'products.product.value_unavailable' | t: option_value: '[value]' }}`
            };
            window.accessibilityStrings = {
                imageAvailable: `{{ 'products.product.media.image_available' | t: index: '[index]' }}`,
                shareSuccess: `{{ 'general.share.success_message' | t }}`,
                pauseSlideshow: `{{ 'sections.slideshow.pause_slideshow' | t }}`,
                playSlideshow: `{{ 'sections.slideshow.play_slideshow' | t }}`
            };{% endcomment %}const throttle = (func, limit) => {
            let inThrottle;
            return function() {
                const args = arguments;
                const context = this;
                if (! inThrottle) {
                    func.apply(context, args);
                    inThrottle = true;
                    setTimeout(() => {
                        inThrottle = false;
                    }, limit);
                }
            };
            };
        </script>
        <script>
            if (templateName == 'index') {
                document.querySelector('.header__logo').addEventListener('click', function() {
                    window.scrollTo(0, 0);
                });
            } else {
                document.querySelector('.header__logo').addEventListener('click', function() {
                    window.location.href = '/';
                });
            }[...document.querySelectorAll('.hero__content, .header__logo, .cart-toggle')].forEach((item) => item.classList.add('active'));
        </script>
        <script type="application/json" data-ajax-cart-initial-state>
            {{ cart | json }}
        </script>
        <script type="module">
            import '{{ "liquid-ajax-cart-v1.10.3.js" | asset_url }}';
        </script>
        {% comment %} <script src="{{ 'random-lazyloader.js' | asset_url }}" defer></script> {% endcomment %}
        <script type="lazyload_int" data-src="{{ 'lazyload.js' | asset_url }}"></script>
        <script type="lazyload_int" data-src="{{ 'wheelio.js' | asset_url }}"></script>
        {% if page.handle == 'faqs' %}
            <script src="{{ 'faq.js' | asset_url }}" defer></script>
        {% endif %}
	    {{ shop.metafields.loox["global_html_body"] | replace: '<script async src="' , '<script type="lazyload_int" data-src="' }}
        {% render 'wnw_footer' %}
        <script>
            var site_nav_link_burger = true;
            document.addEventListener("DOMContentLoaded", function() {
                var icon_menu = document.querySelector(".menu-toggle");
                icon_menu.addEventListener("click", function() {
                    if(seal-div-icon) {
                        event.preventDefault();
                        site_nav_link_burger = false;
                    }
                });
            })
        </script>
        {% if product %}
            {% render 'klaviyo' %}
        {% endif %}
    </body>
</html>